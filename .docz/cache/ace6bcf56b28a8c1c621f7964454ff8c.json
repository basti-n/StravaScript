JJ;AAUA,SACE,oBAAC,UAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE,oBAAC,gBAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAKV,QAAQ,CAACc,IAAd,CADF,EAEE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAIb,iBAAJ,CAFF,CADF,EAKE,oBAAC,cAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE,6CAASG,aAAT;AAAwB,IAAA,GAAG,EAAC,UAA5B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KADF,EAEE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAIE,YAAJ,CAFF,EAGE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAQM,aAAR,CAHF,CALF,CADF;AAaD;;;;;;;;;;;;;AAEDb,YAAY,CAACgB,SAAb,GAAyB;AACvBf,EAAAA,QAAQ,EAAE5B,SAAS,CAAC4C,KAAV,CAAgB;AACxBT,IAAAA,YAAY,EAAEnC,SAAS,CAAC6C,MADA;AAExBJ,IAAAA,SAAS,EAAEzC,SAAS,CAAC8C,OAAV,CAAkB9C,SAAS,CAAC+C,MAA5B,CAFa;AAGxBL,IAAAA,IAAI,EAAE1C,SAAS,CAAC+C,MAHQ;AAIxBjB,IAAAA,UAAU,EAAE9B,SAAS,CAAC+C,MAJE;AAKxBd,IAAAA,IAAI,EAAEjC,SAAS,CAAC+C;AALQ,GAAhB;AADa,CAAzB;AAUApB,YAAY,CAACqB,YAAb,GAA4B;AAC1Bb,EAAAA,YAAY,EAAE,CADY;AAE1BM,EAAAA,SAAS,EAAE,CAAC,SAAD,EAAY,KAAZ,EAAmB,IAAnB,CAFe;AAG1BC,EAAAA,IAAI,EAAE,iBAHoB;AAI1BO,EAAAA,SAAS,EAAE9C,MAAM,GAAG+C,WAAT,EAJe;AAK1BjB,EAAAA,IAAI,EAAE;AALoB,CAA5B;;;;;;;;;;0BAjHM5B,e;0BAYAY,U;0BAOAK,gB;0BAcAE,c;0BAmCkBG,Y","sourcesContent":["import React from 'react'\nimport PropTypes from 'prop-types'\nimport styled from 'styled-components'\nimport Tags from './Tags'\nimport moment from 'moment'\nimport 'moment/locale/de'\nmoment.locale('de')\n\nconst activityTypeImg = {\n  Code: { src: '/assets/code.svg', alt: 'code tag icon' },\n  Ride: { src: '/assets/bike.svg', alt: 'ride icon' },\n  VirtualRide: { src: '/assets/bike.svg', alt: 'ride icon' },\n  Run: { src: './assets/run.svg', alt: 'run icon' },\n  Swim: { src: './assets/google.svg', alt: 'swim icon' },\n  Walk: { src: '/assets/run.svg', alt: 'run icon' },\n  WeightTraining: { src: '/assets/dumbbell.svg', alt: 'weight icon' },\n  Workout: { src: '/assets/dumbbell.svg', alt: 'weight icon' },\n  Yoga: { src: '/assets/yoga.svg', alt: 'yoga icon' },\n}\n\nconst StyledCard = styled.article`\n  background: ${props => props.theme.secondaryColor1};\n  border-radius: 10px;\n  margin-bottom: 20px;\n  padding: 10px 15px;\n`\n\nconst StyledCardHeader = styled.div`\n  align-items: center;\n  border-bottom: 1px solid #000000;\n  display: flex;\n  height: 30px;\n  margin-bottom: 3px;\n  padding: 0 10px 5px;\n  justify-content: space-between;\n  h2,\n  p {\n    font-size: 15px;\n  }\n`\n\nconst StyledCardBody = styled.section`\n  align-items: center;\n  display: grid;\n  font-size: 15px;\n  grid-template-rows: repeat(2, 1fr);\n  grid-template-columns: repeat(2, 1fr);\n  height: fit-content;\n  padding: 0 5px;\n\n  img {\n    grid-row: 1 / 3;\n    padding-left: 5px;\n    width: 75px;\n  }\n  p,\n  label {\n    margin: 5px 0 0 auto;\n  }\n\n  label {\n    align-self: start;\n    color: ${props => props.theme.red};\n    display: grid;\n    grid-template-columns: 1fr max-content;\n    justify-items: end;\n\n    img {\n      grid-column: 2 / -1;\n      margin-top: -5px;\n      margin-left: 5px;\n      width: 25px;\n    }\n  }\n`\n\nexport default function ActivityCard({ activity }) {\n  const activityStartDate = moment(activity.start_date).format('Do MMM YYYY')\n  const activityImage = activityTypeImg[activity.type]\n  const activityTime =\n    activity.elapsed_time < 60\n      ? `${Math.round(activity.elapsed_time)} sec`\n      : `${Math.round(activity.elapsed_time / 60)} min`\n  const activitySportInformation = activity.average_heartrate\n    ? `Avg. HR ${Math.round(activity.average_heartrate)}`\n    : `n/a`\n  const activityLabel =\n    activity.type === 'Code' ? (\n      <Tags languages={activity.languages} />\n    ) : (\n      <>\n        {activitySportInformation}\n        <img src=\"/assets/heart-rate.svg\" alt=\"heartrate monitor\" />\n      </>\n    )\n\n  return (\n    <StyledCard>\n      <StyledCardHeader>\n        <h2>{activity.name}</h2>\n        <p>{activityStartDate}</p>\n      </StyledCardHeader>\n      <StyledCardBody>\n        <img {...activityImage} alt=\"activity\" />\n        <p>{activityTime}</p>\n        <label>{activityLabel}</label>\n      </StyledCardBody>\n    </StyledCard>\n  )\n}\n\nActivityCard.propTypes = {\n  activity: PropTypes.shape({\n    elapsed_time: PropTypes.number,\n    languages: PropTypes.arrayOf(PropTypes.string),\n    name: PropTypes.string,\n    start_date: PropTypes.string,\n    type: PropTypes.string,\n  }),\n}\n\nActivityCard.defaultProps = {\n  elapsed_time: 0,\n  languages: ['backend', 'css', 'js'],\n  name: 'Coding Activity',\n  startDate: moment().toISOString(),\n  type: 'Code',\n}\n"]}]}